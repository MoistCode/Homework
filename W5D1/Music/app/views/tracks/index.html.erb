<h1>List of all tracks. There's quite a bit.</h1>

<% Album.all.each do |album| %>
  <ol>
    <h3><a href="<%= album_url(album) %>">Album: <%= album.title %></a></h3>

    <% album.tracks.each do |track| %>
      <a href="<%= track_url(track) %>"><%= track.title %></a>
      <br>
    <% end %>
  </ol>
<% end %>
